@model List<TbRol>
<div class="card">
    <div class="card-header">
    <h4>Tabla de Roles</h4>
    </div>
    <div class="card-body">
    <table class="table table-striped"> 
        <thead>
            <tr>
                <th>IdRol</th>
                <th>Rol</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var tbRol in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>tbRol.IdRol)</td>
                <td>@Html.DisplayFor(m=>tbRol.NombreRol)</td>
            </tr>
        }
        </tbody>
    </table>
    </div>
</div>